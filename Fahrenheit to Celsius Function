void printTable(int start, int end, int step) { 
    int currentValue = start; 
        while(currentValue <= end){ 
        
        int fahrenheitValue = (int)((5.0 / 9) * (currentValue - 32)); 
        cout << currentValue << "\t" << fahrenheitValue << endl; 
        currentValue += step; 
        
        } 
}
