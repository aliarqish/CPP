// input - given string
// Update in the given input itself. No need to return or print anything

void reverse(char* input,char* end)
{
	char temp;
	while(input < end)
	{
		temp = *input;
		*input++ = *end;
		*end-- = temp;
	}
}



void reverseEachWord(char* input) {
    // Write your code here
char* word_begin = input;
	char* temp = input;

	// int length = strlen(input);

	while(*temp)
	{
		temp++;
		if(*temp == '\0')
		{
			reverse(word_begin,temp-1);
		}
		else if(*temp == ' ')
		{
			reverse(word_begin,temp-1);
			word_begin = temp+1;
		}
	}
}

