void Rotate(int arr[], int d, int size) {
    
int i,count=0;
     int ans[size];
     for(i=0;i<size;i++){
        ans[i]=arr[i];
     }
     for(i=d;i<size;i++){
        arr[count]=arr[i];
        count++;
     }
    while(count<size){
            for(i=0;i<d;i++){
            arr[count]=ans[i];
            count++;
            }
    }
}	
