#include<stdio.h>
#include<string.h>
void removeAllOccurrencesOfChar(char input[], char c) {
    // Write your code here
int length=strlen(input);
    int i=0,j=0;

    for(i=0;i<length;i++){

            if(input[i]==c){
                for(j=i;j<length;j++){
                    input[j]=input[j+1];
                }
              i--;
            }
    }
}
