#include <iostream>
using namespace std;
int main() {
    int col,row,n,value;
    cin>>n;
    row=1;

    while(row<=n){
        col=1,value=row;

            while(col<=row){
                cout<<value;
                value=value+1;
                col=col+1;
            }
            cout<<"\n";
            row=row+1;
    }
}
