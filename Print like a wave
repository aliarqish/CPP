#include <iostream>
using namespace std;

void wavePrint(int input[][1000], int row, int col){
	
  int i,j;
    for(j=0;j<col;j++){
            if(j%2==0){
        for(i=0;i<row;i++){
            cout<<input[i][j]<<" ";
        }
    }else{
        for(i=row-1;i>=0;i--){
            cout<<input[i][j]<<" ";
        }

    }
  }

}

