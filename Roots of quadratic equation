#include<iostream>
#include<math.h>

using namespace std;

int main(){
    int a,b,c,d,r1,r2;
    cin>>a>>b>>c;
    d=((b*b)-(4*a*c));

    if(d==0){
        r1=round((-b+sqrt(d))/(2*a));
        r2=round((-b+sqrt(d))/(2*a));
        cout<<"0";
        cout<<"\n";
        cout<<r1<<" "<<r2;

    }else if(d>0){
        r1=round((-b+sqrt(d))/(2*a));
        r2=round((-b-sqrt(d))/(2*a));
        cout<<"1";
        cout<<"\n";
        cout<<r1<<" "<<r2;
    }else{
        cout<<"-1";
    }

}
